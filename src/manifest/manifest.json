{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.9/MicrosoftTeams.schema.json",
  "manifestVersion": "1.9",
  "id": "{{APPLICATION_ID}}",
  "version": "{{VERSION}}",
  "packageName": "{{PACKAGE_NAME}}",
  "developer": {
    "name": "Engage Squared",
    "websiteUrl": "https://{{PUBLIC_HOSTNAME}}",
    "privacyUrl": "https://{{PUBLIC_HOSTNAME}}/privacy.html",
    "termsOfUseUrl": "https://{{PUBLIC_HOSTNAME}}/tou.html"
  },
  "name": {
    "short": "Room Check-In",
    "full": "Room Check-In"
  },
  "description": {
    "short": "Optus Check-In App is a Teams app to manage room bookings and meeting attendance.",
    "full": "Optus Check-In App is a Teams app to manage room bookings and meeting attendance. Meeting hosts and check themselves and guests into a booked room, whilst ensuring they do not exceed COVID-19 capacity limits. Attendance data is stored and can be accessed by Admins in case of a COVID-19 case."
  },
  "icons": {
    "outline": "icon-outline.png",
    "color": "icon-color.png"
  },
  "accentColor": "#D85028",
  "configurableTabs": [],
  "staticTabs": [
    {
      "entityId": "cd95eda0-fd58-4e81-bc94-95e902a4b0e9",
      "scopes": ["personal"],
      "context":["personalTab"],
      "name": "Location Checkin",
      "contentUrl": "https://{{PUBLIC_HOSTNAME}}/roomCheckInTab",
      "websiteUrl": "https://{{PUBLIC_HOSTNAME}}/roomCheckInTab"
    }
  ],
  "bots": [],
  "connectors": [],
  "composeExtensions": [],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "{{PUBLIC_HOSTNAME}}"
  ],
  "showLoadingIndicator": true,
  "isFullScreen": false,
  "webApplicationInfo": {
    "id": "{{AUTH_APP_CLIENTID}}",
    "resource": "{{AUTH_APP_URI}}"
  }
}
